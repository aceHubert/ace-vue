{
  "name": "acevue",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,,.vue --ignore-path .gitignore ."
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.7.4",
    "@nuxt/babel-preset-app": "^2.10.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": ">=1",
    "jest": "^24.9.0",
    "lint-staged": ">=8",
    "nodemon": "^1.19.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 9"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "workspaces": [
    "packages/*",
    "docs"
  ]
}
